@import "../../static/css/font.wxss";
.record-wrap{
    width:100%;
    box-sizing: border-box;
    /*border: 1px solid red;*/
    margin:2px 0;
}
.record-title{
    height: 48px;
    line-height: 48px;
    border-radius:5px;
    padding:0 20px;
    box-sizing: border-box;
    border: 2px solid;
}
.record-content{
    padding:20px 30px;
}
.record-0 .record-title{
   color: #ffffcc;
   border-color: #ffffcc;
}
.record-1 .record-title{
   color: #ccffff;
    border-color: #ccffff;
}
.record-2 .record-title{
   color: #ffcccc;
    border-color: #ffcccc;
}
.record-3 .record-title{
   color: #ffcc99;
    border-color: #ffcc99;
}
.record-4 .record-title{
   color: #99cccc;
    border-color: #99cccc;
}
.record-5 .record-title{
   color: #ccff99;
    border-color: #ccff99;
}
.record-6 .record-title{
   color: #99cc99;
